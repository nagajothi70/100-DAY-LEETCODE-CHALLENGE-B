class Solution {
    public boolean checkIfExist(int[] arr) {
        int len=arr.length;
        for(int i = 0;i<len;i++){
            for(int j =len-1;j>=0;j--){
                if(i!=j){
                 
                if(arr[i]==2*arr[j]){
return true;
                }
            
            
                }
            }

        }
        return false;
    }
}
